{
  "name": "gatsby-starter-tractstack",
  "description": "Tract Stack by At Risk Media gatsby starter",
  "version": "0.1.0",
  "author": "Adon Irani <a@atriskmedia.com>",
  "license": "FSL-1.0-MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/at-risk-media/gatsby-starter-tractstack"
  },
  "bugs": {
    "url": "https://gitlab.com/at-risk-media/gatsby-starter-tractstack/issues"
  },
  "keywords": [
    "tractstack",
    "gatsby",
    "starter",
    "typescript",
    "tailwind"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"./*.+(ts|js|tsx)\" \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@tractstack/helpers": "0.1.16",
    "@tractstack/types": "0.1.3",
    "gatsby": "5.12.12",
    "gatsby-plugin-image": "3.12.3",
    "gatsby-plugin-manifest": "5.12.3",
    "gatsby-plugin-no-sourcemaps": "5.12.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-sharp": "5.12.3",
    "gatsby-plugin-sitemap": "6.12.3",
    "gatsby-plugin-styled-components": "6.12.0",
    "gatsby-plugin-tractstack": "1.4.35",
    "gatsby-source-drupal": "6.12.3",
    "gatsby-source-filesystem": "5.12.1",
    "gatsby-source-shopify": "8.12.3",
    "gatsby-transformer-remark": "6.12.3",
    "gatsby-transformer-sharp": "5.12.3",
    "iframe-resizer-react": "1.1.0",
    "react": "18.2.0",
    "react-cool-inview": "3.0.1",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-player": "2.14.1",
    "shopify-buy": "2.21.1",
    "styled-components": "6.1.1",
    "vis-data": "7.1.9",
    "vis-network": "9.1.9",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@types/react-helmet": "6.1.11",
    "@types/shopify-buy": "2.17.4",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "autoprefixer": "10.4.16",
    "babel-plugin-styled-components": "2.1.4",
    "babel-preset-gatsby": "3.12.1",
    "c8": "8.0.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "gatsby-plugin-postcss": "6.12.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "23.0.1",
    "lint-staged": "15.2.0",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "tailwindcss": "3.3.6",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.3.3"
  }
}
