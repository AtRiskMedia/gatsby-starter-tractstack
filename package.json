{
  "name": "gatsby-starter-tractstack",
  "description": "Tract Stack by At Risk Media gatsby starter",
  "version": "1.1.0",
  "author": "Adon Irani <a@atriskmedia.com>",
  "private": true,
  "license": "SEE LICENSE IN <LICENSE>",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/at-risk-media/gatsby-starter-tractstack"
  },
  "bugs": {
    "url": "https://gitlab.com/at-risk-media/gatsby-starter-tractstack/issues"
  },
  "keywords": [
    "gatsby",
    "starter",
    "typescript",
    "tailwind",
    "tractstack"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"./*.+(ts|js|tsx)\" \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
