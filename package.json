    "gatsby-transformer-sharp": "5.7.0",
    "gatsby-transformer-tractstack": "1.1.0",
    "iframe-resizer-react": "1.1.0",
    "react": "18.2.0",
    "react-cool-inview": "3.0.1",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
